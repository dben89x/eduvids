<p id="notice"><%= notice %></p>

<h1>Quiz Attempts</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @quiz_attempts.each do |quiz_attempt| %>
      <tr>
        <td><%= link_to 'Show', quiz_attempt %></td>
        <td><%= link_to 'Edit', edit_quiz_attempt_path(quiz_attempt) %></td>
        <td><%= link_to 'Destroy', quiz_attempt, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Quiz Attempt', new_quiz_attempt_path %>
