%p#notice= notice
%h1 Users
%table
  %thead
    %tr
      %th Email
      %th Encrypted password
      %th Reset password token
      %th Reset password sent at
      %th Remember created at
      %th Created at
      %th Updated at
      %th{:colspan => "3"}
  %tbody
    - @users.each do |user|
      %tr
        %td= user.email
        %td= user.encrypted_password
        %td= user.reset_password_token
        %td= user.reset_password_sent_at
        %td= user.remember_created_at
        %td= user.created_at
        %td= user.updated_at
        %td= link_to 'Show', user
        %td= link_to 'Edit', edit_user_path(user)
        %td= link_to 'Destroy', user, method: :delete, data: { confirm: 'Are you sure?' }
%br/
= link_to 'New User', new_user_path
